Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "#1a1a1a"
       , fgColor = "#d0d0d0"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run Weather "LEBL" ["-t","<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Network "enp0s31f6" ["-t","Eth: <rx>/<tx>","-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Network "wlp0s20f3" ["-t","WiFi: <rx>/<tx>","-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Com "uname" ["-s","-r"] "" 36000
                    , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% | %cpu% | %memory% * %swap% | %enp0s31f6% | %wlp0s20f3% }{<fc=#ee9a00>%date%</fc> | %LEBL% | %uname%"
       }